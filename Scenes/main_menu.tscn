	pass

func _physics_process(delta):
		
	angulo += PI/8
	self.global_position = equilibro + Vector2(0, 4*sin(angulo * delta * 15 + deslocamento))
	# Called every frame. Delta is time since last frame.
	# Update game logic here.
	pass
